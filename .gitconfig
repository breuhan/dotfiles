[user]
    name = Michael Contento
    email = michaelcontento@gmail.com
[alias]
    cm = commit -m 
    cma = commit -a -m
    lg = log --graph --pretty=format:'%C(red)%h %C(bold blue)<%an> %C(green)(%cd)%C(reset) %s%C(yellow)%d%Creset' --abbrev-commit --date=relative -- 
    pl = pull --rebase
    st = status -sb
    unstage = reset HEAD --
    who = shortlog -s --
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
[branch]
    autosetupmerge = true
[core]
    autocrlf = false
[push]
    default = tracking
[url "git@github.com:"]
    insteadOf = "gh:"
    pushInsteadOf = "github:"
    pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
    insteadOf = "github:"
[url "git@gist.github.com:"]
    insteadOf = "gst:"
    pushInsteadOf = "gist:"
    pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
    insteadOf = "gist:"
[url "git@heroku.com:"]
    insteadOf = "heroku:"
