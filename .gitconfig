[user]
    name = Michael Contento
    email = michaelcontento@gmail.com
[alias]
    cm = commit --message 
    cma = commit --all --message
    co = checkout
    logg = log --left-right --graph  
    st = status --short --branch
    pl = pull --rebase
    unstage = reset HEAD --
    who = !git shortlog --summary --email | sort -r
[log]
    abbrevCommit = true
    date = relative
[format]
    pretty = default 
[pretty]
    changelog = * %s
    default = %C(red)%h %C(bold blue)<%an> %C(green)(%cd)%C(reset) %s%C(yellow)%d%Creset
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
[branch]
    autosetupmerge = true
[merge]
    summary = true
[core]
	excludesfile = ~/.gitexcludes
    autocrlf = false
[push]
    default = tracking
[url "git@github.com:"]
    insteadOf = "gh:"
    pushInsteadOf = "github:"
    pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
    insteadOf = "github:"
[url "git@gist.github.com:"]
    insteadOf = "gst:"
    pushInsteadOf = "gist:"
    pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
    insteadOf = "gist:"
[url "git@heroku.com:"]
    insteadOf = "heroku:"
[gitflow "branch"]
    master = master
    develop = develop
[gitflow "prefix"]
    feature = feature/
    release = release/
    hotfix = hotfix/
    support = support/
    versiontag = RELEASE_
